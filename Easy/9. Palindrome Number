
#simple approach
class Solution:
    def isPalindrome(self, x: int) -> bool:
        res = 0
        num = x
        if x<0:
            return False
        while x>0:
            last = x%10
            res=res*10+last
            x=x//10
        
        print(res)
        if res==num:
            return True
            
#using slicing
        if x<0:
            return False
        if str(x) == str(x)[::-1]:
            return True
            
 
