#simple approach
class Solution:
    def removeElement(self, nums: List[int], val: int) -> int:
        i=0
        while i< len(nums):
            if nums[i]==val:
                nums.pop(i)
            else:
                i+=1
        return len(nums)


# 2 pointer approach
for i in range(len(nums)):
            
            if (nums[i]!=val):
                # start=start+1
                nums[start] = nums[i]
                start+=1
                print(nums)
        
        return start
